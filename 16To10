import java.math.BigInteger;
import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        while(in.hasNext()){
            if(in.next().length()>=2){
                System.out.println((new BigInteger(in.next().substring(2),16)).toString());
            }
        }
        in.close();
    }
}
